{"nbformat":4,"nbformat_minor":0,"metadata":{"colab":{"provenance":[],"authorship_tag":"ABX9TyORGrXyCM1k9HUqI7Tp3eza"},"kernelspec":{"name":"python3","display_name":"Python 3"},"language_info":{"name":"python"}},"cells":[{"cell_type":"code","execution_count":1,"metadata":{"id":"W5O9Vq7rG_mT","executionInfo":{"status":"ok","timestamp":1682998551489,"user_tz":420,"elapsed":1336,"user":{"displayName":"Andrew Cruz","userId":"01313162568893261575"}}},"outputs":[],"source":["import pandas as pd\n","import numpy as np\n","import requests\n","from bs4 import BeautifulSoup"]},{"cell_type":"code","source":["# https://www.baseball-reference.com/teams/OAK/2020-schedule-scores.shtml"],"metadata":{"id":"230n0kIWIYLX"},"execution_count":null,"outputs":[]},{"cell_type":"code","source":["def get_games(team):\n","    team_df = pd.DataFrame(columns=['date', 'box', 'team', 'at', 'opponent', 'w_or_l', 'runs', 'runs_allowed',\n","                                    'innings', 'record', 'div_rank', 'gb', 'winning_pitcher', 'losing_pitcher',\n","                                    'save', 'time', 'd_or_n', 'attendance', 'streak', 'double_header', 'runs_pg',\n","                                    'runs_ma', 'runs_allowed_ma', 'ten_game', 'opening_day'])\n","    for year in range (1990, 2020):\n","        df = pd.DataFrame(columns=['date', 'box', 'team', 'at', 'opponent', 'w_or_l', 'runs', 'runs_allowed',\n","                            'innings', 'record', 'div_rank', 'gb', 'winning_pitcher', 'losing_pitcher',\n","                            'save', 'time', 'd_or_n', 'attendance', 'cli', 'streak', 'reschedule'])\n","        html = requests.get('http://www.baseball-reference.com/teams/' + team + '/' + str(year) +\n","                            '-schedule-scores.shtml')\n","        bs = BeautifulSoup(html.text, 'html.parser')\n","        for game in bs.find('table', {'class':'stats_table'}).find_all('tr'):\n","            results = []\n","            for element in game.find_all('td'):\n","                results.append(element.text)\n","            if len(results) == 21:\n","                df.loc[len(df)] = results\n","\n","        # check if double header\n","        dh = '('\n","        df['double_header'] = [1 if dh in x else 0 for x in df.date]\n","\n","        # add name of the day\n","        df['day_of_week'] = [x.split(', ')[0] for x in df.date]\n","        \n","        # convert date to datetime object\n","        df.date = df.date.str.replace(r\"\\(.*\\)\",\"\", regex=True)\n","        date = []\n","        for i in df.date:\n","            split = i.split(', ')\n","            date.append(split[1] + ', ' + str(year))\n","        df.date = date\n","        df.date = pd.to_datetime(date)\n","\n","        # shift record, div_rank, gb, streak by one game\n","        df.record = df.record.shift()\n","        df.record.fillna('0-0', inplace=True)\n","        for col in ['div_rank', 'gb', 'streak']:\n","            df[col] = df[col].shift()\n","            df[col].fillna('0', inplace=True)\n","\n","        # moving average for runs and runs allowed\n","        df['runs_pg'] = [0 if x == min(df.date) else df[df.date < x].runs.astype(int).mean() for x in df.date]\n","        df['runs_ma'] = df.runs.rolling(5).mean().shift()\n","        df['runs_allowed_ma'] = df.runs_allowed.rolling(5).mean().shift()\n","        df.runs_ma.fillna(df.iloc[5].runs_ma, inplace=True)\n","        df.runs_allowed_ma.fillna(df.iloc[5].runs_allowed_ma, inplace=True)\n","        \n","        # win_pct of last ten games\n","        ten_game = [list(df.w_or_l[i-10:i]).count('W') / 10. for i in range(10, len(df))]\n","        for i in range(10):\n","            ten_game.insert(i, ((list(df.w_or_l[0:i])).count('W') / (i + 1)))\n","        df['ten_game'] = ten_game\n","\n","        # only home games\n","        df = df[~df['at'].str.contains('@')].reset_index(drop=True)\n","\n","        # opening day var\n","        df['opening_day'] = [1 if x == min(df.date) else 0 for x in df.date]\n","\n","        # add year to the complete data\n","        team_df = pd.concat([team_df, df]).reset_index(drop=True)\n","\n","    return team_df"],"metadata":{"id":"bBIxj_zVHHDF","executionInfo":{"status":"ok","timestamp":1682998650419,"user_tz":420,"elapsed":658,"user":{"displayName":"Andrew Cruz","userId":"01313162568893261575"}}},"execution_count":5,"outputs":[]},{"cell_type":"code","source":["df = get_games('OAK')\n","df.to_csv('OAK_data.csv', index=False, encoding='utf-8')"],"metadata":{"id":"v2Z5-0olHN4W","colab":{"base_uri":"https://localhost:8080/","height":318},"executionInfo":{"status":"error","timestamp":1682998672310,"user_tz":420,"elapsed":155,"user":{"displayName":"Andrew Cruz","userId":"01313162568893261575"}},"outputId":"2f698028-8cca-4041-cd50-842c0622471c"},"execution_count":6,"outputs":[{"output_type":"error","ename":"AttributeError","evalue":"ignored","traceback":["\u001b[0;31m---------------------------------------------------------------------------\u001b[0m","\u001b[0;31mAttributeError\u001b[0m                            Traceback (most recent call last)","\u001b[0;32m<ipython-input-6-c180dda4705e>\u001b[0m in \u001b[0;36m<cell line: 1>\u001b[0;34m()\u001b[0m\n\u001b[0;32m----> 1\u001b[0;31m \u001b[0mdf\u001b[0m \u001b[0;34m=\u001b[0m \u001b[0mget_games\u001b[0m\u001b[0;34m(\u001b[0m\u001b[0;34m'OAK'\u001b[0m\u001b[0;34m)\u001b[0m\u001b[0;34m\u001b[0m\u001b[0;34m\u001b[0m\u001b[0m\n\u001b[0m\u001b[1;32m      2\u001b[0m \u001b[0mdf\u001b[0m\u001b[0;34m.\u001b[0m\u001b[0mto_csv\u001b[0m\u001b[0;34m(\u001b[0m\u001b[0;34m'OAK_data.csv'\u001b[0m\u001b[0;34m,\u001b[0m \u001b[0mindex\u001b[0m\u001b[0;34m=\u001b[0m\u001b[0;32mFalse\u001b[0m\u001b[0;34m,\u001b[0m \u001b[0mencoding\u001b[0m\u001b[0;34m=\u001b[0m\u001b[0;34m'utf-8'\u001b[0m\u001b[0;34m)\u001b[0m\u001b[0;34m\u001b[0m\u001b[0;34m\u001b[0m\u001b[0m\n","\u001b[0;32m<ipython-input-5-c78a7b5e1bbb>\u001b[0m in \u001b[0;36mget_games\u001b[0;34m(team)\u001b[0m\n\u001b[1;32m     11\u001b[